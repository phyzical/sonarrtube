{
  "name": "sonarrtube",
  "version": "0.0.1",
  "description": "Adds shows to various show sources (Youtube)",
  "license": "MIT",
  "preferGlobal": true,
  "type": "module",
  "dependencies": {
    "dotenv": "16.4.7",
    "jimp": "1.6.0",
    "node-fetch": "3.3.2",
    "puppeteer": "24.4.0",
    "puppeteer-extra": "3.3.6",
    "puppeteer-extra-plugin-adblocker": "2.13.6",
    "puppeteer-extra-plugin-stealth": "2.11.2",
    "source-map-support": "0.5.21",
    "tesseract.js": "6.0.0",
    "sharp": "0.33.5"
  },
  "scripts": {
    "build": "tsc --p tsconfig.build.json && tsc-alias",
    "watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "test": "jest && npm run badge",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "start": "node main.js",
    "badge": "jest-coverage-badges -output ./badges"
  },
  "devDependencies": {
    "@faker-js/faker": "9.6.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.14.0",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "concurrently": "9.1.2",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.17.0",
    "http-request-mock": "2.0.2",
    "jest-coverage-badges": "1.1.2",
    "jest": "29.7.0",
    "jest-resolve": "29.7.0",
    "jest-puppeteer": "11.0.0",
    "jest-extended": "4.0.2",
    "jest-ts-webcompat-resolver": "1.0.0",
    "ts-jest": "29.3.1",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.11",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0"
  }
}