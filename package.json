{
  "name": "show-submitter",
  "version": "0.0.1",
  "description": "Adds shows to various show sources (Youtube)",
  "license": "MIT",
  "preferGlobal": true,
  "type": "module",
  "dependencies": {
    "puppeteer": "22.12.1",
    "dotenv": "16.4.5",
    "node-fetch": "3.3.2",
    "webp-converter": "2.3.3",
    "puppeteer-extra": "3.3.6",
    "puppeteer-extra-plugin-adblocker": "2.13.6",
    "puppeteer-extra-plugin-stealth": "2.11.2",
    "tesseract.js": "5.1.0",
    "jimp": "0.22.12",
    "source-map-support": "0.5.21"
  },
  "scripts": {
    "build": "tsc --p tsconfig.build.json",
    "watch": "tsc -w --p tsconfig.build.json",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "start": "node main.js"
  },
  "devDependencies": {
    "typescript": "5.5.3",
    "@typescript-eslint/parser": "7.15.0",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "typescript-eslint": "7.15.0",
    "eslint": "8.57.1",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-n": "17.10.3",
    "jest": "29.7.0",
    "ts-jest": "29.1.5",
    "ts-node": "10.9.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.9"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}